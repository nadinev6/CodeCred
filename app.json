{
  "expo": {
    "name": "CodeCred",
    "slug": "code-cred",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/1755687723565.png",
    "scheme": "codecred",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "flipper": false
          },
          "android": {
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}